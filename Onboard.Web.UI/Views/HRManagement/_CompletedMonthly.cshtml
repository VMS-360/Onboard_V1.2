@model List<List<string>>

@if (Model.Count > 0)
{
    <table class="table table-hover" id="Completed-Monthly-Table">
        <thead>
            <tr>
                @foreach (var header in Model[0])
                {
                    <th class="project-title">
                        @header
                    </th>
                }
            </tr>
        </thead>
        <thead>
            @for (int i=1; i<Model.Count; i++)
            {
                <tr>
                    @foreach (var theRow in Model[i])
                    {
                        <td>
                            @theRow
                        </td>
                    }
                </tr>
            }
        </thead>
    </table>
}