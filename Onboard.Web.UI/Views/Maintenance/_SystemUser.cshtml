@model IEnumerable<Onboard.Web.UI.Models.MaintenanceViewModels.OnboardUserViewModel>
    <div class="row m-b-sm m-t-sm">
        <div class="col-md-6">
            <div class="input-group">
                <input type="text" placeholder="Search" class="input-sm form-control"> <span class="input-group-btn">
                    <button type="button" class="btn btn-sm btn-primary"> Go!</button>
                </span>
            </div>
        </div>
    </div>

    <div class="project-list">
        <table class="table table-hover">
            <tbody>
                @foreach (var user in Model)
                    {
                    <tr>
                        <td class="project-title">
                            @user.FirstName @user.LastName
                        </td>
                        <td class="project-title">
                            @user.Email
                        </td>
                        <td class="project-title">
                            @user.RoleString
                        </td>
                        <td class="project-title">
                            @user.InternalString
                        </td>
                        <td class="project-actions">
                            <a href="#" class="btn btn-white btn-sm"><i class="fa fa-folder"></i> View </a>
                            <a href="#" class="btn btn-primary btn-sm"><i class="fa fa-edit"></i> Edit </a>
                        </td>
                    </tr>
            }
            </tbody>
        </table>
    </div>
