@model IEnumerable<Onboard.Web.UI.Models.DatabaseViewModels.EndClientsViewModel>

<table class="table table-hover" id="EndClient-Lists">
    <tbody>
        @foreach (var endclient in Model)
        {
            <tr pk-id="@endclient.EndClientId">
                <td class="project-title">
                    @if (endclient.Status == "A")
                    {
                        <span class="label label-success">Active</span>
                    }
                    else
                    {
                        <span class="label label-danger">Inactive</span>
                    }
                </td>
                <td class="project-title">
                    @endclient.CompanyName
                </td>
                <td class="project-actions">
                    <a href="#" class="btn btn-white btn-sm" action="edit-client"><i class="fa fa-edit"></i> Edit </a>
                    <a href="#" class="btn btn-white btn-sm" action="view-client"><i class="fa fa-folder"></i> View </a>
                </td>
            </tr>
        }
    </tbody>
</table>